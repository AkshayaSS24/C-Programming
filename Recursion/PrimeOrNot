#include<stdio.h>
int n, i;
int main(){
	int Prime(int, int);
	int result;
	printf("Enter the number to check if it is prime or not :");
	scanf("%d", &n);
	result = Prime(n,n/2);
	if(result==1)
	printf("It is a prime number");
	else
	printf("It is not a Prime number");
}

int Prime(int n, int i)
{

	if(i==1)
	return 1;
	else if(n%i==0)	
	return 0;
	else
	{
	  return 1;
	  Prime(n, i-1);
}
}
	

	
		
	
	
